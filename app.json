{
  "expo": {
    "name": "Toledo Extintores",
    "slug": "toledo-extintores",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/extintor.jpg",
    "scheme": "toledoextintores",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/extintor.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.oeduardodutra.toledoextintores"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/extintor.jpg"
    },
    "plugins": ["expo-router"],

    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5624f1e5-f331-4e3c-bd38-4f8507d327cb"
      }
    },
    "owner": "oeduardodutra"
  }
}
